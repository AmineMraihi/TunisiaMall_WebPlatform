




{% include "TunisiaMallBundle:shop:templateshopowner.html.twig" %}
{% block robots %}
    <section id="content">
        <link rel="stylesheet" href="{{ asset('assets/css/vendor/promotion.css') }}">
        <div class="page page-tables-datatables">

            <div class="pageheader">
                <br>


                <div id="titreajout">Ajouter une promotion</div>


            </div>

            <!-- row -->
            <div class="row">
                <!-- col -->
                <div class="col-md-12">


                    <section class="tile">

                        <!-- tile header -->
                        <div >


                        </div>
                        <!-- /tile header -->



                        <div class="content">
                            {{ form_start(formulaire) }}

                            <div>

                                {{ form_label(formulaire.taux_reduction) }} :
                                {{ form_widget( formulaire.taux_reduction,{'attr': {'class':'form-control'}}   ) }}
                            </div>


                            <div>

                                {{ form_label(formulaire.date_debut) }} :
                                {{ form_widget( formulaire.date_debut,{'attr': {'class':'form-control'}}   ) }}
                            </div>





                            <div>
                                {{ form_label(formulaire.date_fin) }} :
                                {{ form_widget( formulaire.date_fin,{'attr': {'class':'form-control'}}   ) }}
                            </div>



                              <div>
                            <{{ form_widget(formulaire.imageFile) }}>
                        </div>




                            <div>

                                Id de boutique :   <span></span>
                                {{ form_widget(formulaire.id_produit)}}

                            </div>


                            <div>
                                {{ form_widget( formulaire.ajouter,{'attr': {'class':'form_ajouter'}}  ) }}

                            </div>
                            {{ form_end(formulaire) }}

                        </div>

                        <!-- /tile body -->

                    </section>
                </div>
            </div>
        </div>


    </section>





    <!--/ Page Specific Scripts -->
